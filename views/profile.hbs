<div id="profilePage">
<h1>Profile page of: {{user.username}}</h1> 
<br><br>
<h3>Search movies to add to my Favourites</h3>
<a href="/search-movie" id="searchButton">Search</a>


</div>
<br><br><br>
<h2>My favorites movies</h2>


<div id="favList">

	<div id="favMovie">
	{{#each listToPass.movies}}

	<img src="{{posterUrl}}"  width='100px' alt="Poster for {{movie_title}}">
	<h4>{{movie_title}}</h4>
	<h4>Score: {{ApiRating}}</h4>
	<a href="/movie/delete?movie_id={{movie_Id}}" id="remove">Remove from my favorites</a>



	<br><br><br>
	{{/each}}

</div>


</div>


