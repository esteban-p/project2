<h1>Profile page of: {{user.username}}</h1> 



<form action="/profile" method="POST">
	<label for="listName">New list name: </label>
	<input type="text" name="listName">

	<button type="submit">Create a new list</button>
</form>

<br><br>
<h3>My Lists</h3>




{{#each lists}}
    <br><br><br>
	<a href="/lists/{{ _id }}">{{ title }}</a>
    
	{{#each movies}}
	<h4>{{movie_Id}}</h4>
	{{/each}}

{{/each}}




