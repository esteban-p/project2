
<div id="profileButton">
    <a href=“/profile/{{user._id}}“ id="profileLink">Back to Profile</a>
</div>
<div id="searchBar">
<h1> Type name of Movie</h1>
<form action="/movie-search" method="GET">
<input type="text" name='movie' id='movie' placeholder="Movie Name">
<button type="submit" id="searchBtn">Search</button> 
</form>
</div>



<div id="searchResults">
{{#each results}}

    <div id="result">
    {{> movieCard this}}
    </div>

{{/each}}
</div> 

